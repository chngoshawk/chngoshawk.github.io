<% include ../header %>
    <div class="container">
        <ol class="breadcrumb">
            <li>
                <a href="#">后台管理页面</a>
            </li>
            <li>
                <a href="#">后台列表</a>
            </li>
        </ol>
    </div>
    <div class="row">
        <div class="col-md-3 hidden-sm hidden-xs"></div>
        <div class="col-md-6 col-sm-12">
            <form action="/admin/modified" method="POST" class="dataList">
                <input type="hidden" name="_id" value="<%= dataList._id %>">
                <div class="input-group input-group-lg">
                    <span class="input-group-addon" id="sizing-addon1">排序</span>
                    <input type="number" class="form-control" name="order" value="<%= dataList.order %>">
                </div>
                <div class="input-group input-group-lg">
                    <span class="input-group-addon" id="sizing-addon1">名称</span>
                    <input type="text" class="form-control" name="listname" value="<%= dataList.listname %>">
                </div>
                <div class="input-group input-group-lg">
                    <span class="input-group-addon" id="sizing-addon1">介绍</span>
                    <input type="text" class="form-control" name="intro" value="<%= dataList.intro %>">
                </div>
                <div>
                    <input type="submit" class="data-submit">
                    <a href="/admin/list"><input type="button" value="返回" class="data-submit"></a>
                </div>
            </form>
        </div>
        <div class="col-md-3 hidden-sm hidden-xs"></div>
    </div>
    <% include ../footer %>